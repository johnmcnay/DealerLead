
@model IEnumerable<DealerLead.SupportedState>

<form method="post" action="/Dealerships/Create">
    <div class="form-group">
        <label>
            Dealership Name
            <input class="form-control" type="text" name="DealershipName" />
        </label>
    </div>
    <div class="form-group">
        <label>
            Street Address 1
            <input class="form-control" type="text" name="StreetAddress1" />
        </label>
    </div>
    <div class="form-group">
        <label>
            Street Address 2
            <input class="form-control" type="text" name="StreetAddress2" />
        </label>
    </div>
    <div class="form-group">
        <label>
            City
            <input class="form-control" type="text" name="City" />
        </label>
    </div>
    <div class="form-group">
        <label>
            State
            <select class="form-control" name="stateId">
                @foreach (var state in Model)
                {
                    <option value="@state.id">@state.Abbreviation</option>
                }
            </select>
        </label>
    </div>
    <div class="form-group">
        <label>
            Zipcode
            <input class="form-control" type="text" name="Zipcode" />
        </label>
    </div>
    <input type="submit" />
</form>

